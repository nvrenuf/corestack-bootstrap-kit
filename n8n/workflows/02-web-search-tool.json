              "name": "Authorization",
              "value": "={{'Bearer ' + $json.search_api_key}}"
            }
          ]
        }
      },
      "id": "http-search",
      "name": "Search API Request (Placeholder)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1420, 180],
      "continueOnFail": true
    },
    {
      "parameters": {
        "jsCode": "const results = [];\nif (Array.isArray($json.results)) {\n  for (const r of $json.results.slice(0, 10)) {\n    results.push({\n      title: String(r.title || ''),\n      url: String(r.url || r.link || ''),\n      snippet: String(r.snippet || r.description || ''),\n      source: String(r.source || 'api'),\n      published_at: r.published_at || null\n    });\n  }\n}\nconst payload = {\n  query: $json.query || '',\n  results,\n  searched_at: new Date().toISOString()\n};\nconsole.log('corestack.web_search', JSON.stringify({ query: payload.query, results_count: payload.results.length }));\nreturn [{ json: payload }];"
      },
      "id": "format-search",
      "name": "Format Search Results",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1660, 180]
    },
    {
      "parameters": {
        "mode": "manual",
        "jsonOutput": "={{ {\"query\": $json.query, \"results\": [], \"searched_at\": new Date().toISOString() } }}"
