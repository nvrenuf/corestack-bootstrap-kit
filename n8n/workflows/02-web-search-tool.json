      "type": "n8n-nodes-base.set",
      "typeVersion": 3.4,
      "position": [460, 320]
    },
    {
      "parameters": {
        "jsCode": "const incoming = $items('Webhook /tools/web.search', 0, 0)[0].json.body || $items('Webhook /tools/web.search', 0, 0)[0].json;\nconst inputs = incoming.inputs || {};\nconst query = String(inputs.query || '').trim();\nconst max_results = Math.min(Math.max(Number(inputs.max_results || 5), 1), 10);\nif (!query) {\n  return [{ json: { ok: false, code: 400, error: 'inputs.query is required' } }];\n}\nconst hasKey = $json.search_api_key && $json.search_api_key !== 'REQUIRED';\nreturn [{ json: { ok: true, query, max_results, has_key: Boolean(hasKey), search_api_url: $json.search_api_url, search_api_key: $json.search_api_key } }];"
      },
      "id": "validate-input",
      "name": "Validate Search Inputs",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [700, 320]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [
            {
              "value1": "={{$json.ok}}",
              "value2": true
            }
          ]
        }
      },
      "id": "if-valid",
      "name": "Valid?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2.2,
      "position": [940, 320]
    },
